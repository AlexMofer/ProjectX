<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DrawableRatingBar">
        <attr name="drbProgressDrawable" format="reference" />
        <attr name="drbSecondaryProgress" format="reference" />
        <attr name="drbDrawablePadding" format="dimension"/>
        <attr name="drbMax" format="integer"/>
        <attr name="drbMin" format="integer"/>
        <attr name="drbRating" format="integer"/>
        <attr name="drbManually" format="boolean"/>
        <attr name="drbOnlyItemTouchable" format="boolean"/>
        <attr name="drbGravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="center" value="0x11" />
        </attr>
    </declare-styleable>
</resources>