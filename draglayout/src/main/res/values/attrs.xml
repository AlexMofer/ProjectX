<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DragLayout_Layout">
        <attr name="alDraggable" format="boolean"/>
        <attr name="dlDragPadding" format="dimension"/>
        <attr name="dlDragPaddingStart" format="dimension"/>
        <attr name="dlDragPaddingTop" format="dimension"/>
        <attr name="dlDragPaddingEnd" format="dimension"/>
        <attr name="dlDragPaddingBottom" format="dimension"/>
        <attr name="dlLayout_gravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="center" value="0x11" />
            <flag name="start" value="0x00800003" />
            <flag name="end" value="0x00800005" />
        </attr>
    </declare-styleable>
</resources>