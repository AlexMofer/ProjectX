<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PowerfulWebView">
        <attr name="pwvProgressColor" format="color" />
        <attr name="pwvProgressHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CircularProgressImageView">
        <attr name="cpStyle" format="enum">
            <enum name="large" value="0" />
            <enum name="normal" value="1" />
        </attr>
        <attr name="cpStrokeWidth" format="dimension" />
        <attr name="cpCenterRadius" format="dimension" />
        <attr name="cpPaddingEdge" format="dimension" />
        <attr name="cpSchemeColor1" format="color" />
        <attr name="cpSchemeColor2" format="color" />
        <attr name="cpSchemeColor3" format="color" />
        <attr name="cpSchemeColor4" format="color" />
        <attr name="cpSchemeColor5" format="color" />
        <attr name="cpAutoStart" format="boolean" />
        <attr name="cpShadowsCircleColor" format="color" />
        <attr name="cpShadowsCircleRadius" format="dimension" />
        <attr name="cpElevation" format="dimension" />
    </declare-styleable>
</resources>